<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class catalog="formula_mind"
  name="au.com.phytoline.entity.BatchDetails" table="batch_details">
  <id name="bdid" type="java.lang.Integer">
   <column name="bdid"/>
   <generator class="native"/>
  </id>
  <property generated="never" lazy="false" name="bserials" type="java.lang.Integer">
   <column name="bserials"/>
  </property>
  <property generated="never" lazy="false" name="pname" type="java.lang.String">
   <column length="100" name="pname"/>
  </property>
  <property generated="never" lazy="false" name="bcode" type="java.lang.String">
   <column length="10" name="bcode"/>
  </property>
  <property generated="never" lazy="false" name="chemName" type="java.lang.String">
   <column length="50" name="chem_name"/>
  </property>
  <property generated="never" lazy="false" name="quantity" type="java.lang.Double">
   <column name="quantity" precision="22" scale="0"/>
  </property>
  <property generated="never" lazy="false" name="qtyRequired" type="java.lang.Double">
   <column name="`qty-required`" precision="22" scale="0"/>
  </property>
 </class>
</hibernate-mapping>
